
Model

model 
{
for (i in 1:N)
	{ 
	for (j in 1:J) 
		{
		delta[i,j] <- equals(risk[i], j)
		}
		for (j in 1:Td)
		{
			R[i,j] <- step(os[i] - a[j] + eps)
			for (k in 1:J) 
			{
				dN[i,j,k] <- R[i,j]*step(a[j+1] - os[i] - eps)*delta[i,k]
                dN[i,j,k] ~ dpois(lambda[i, j, k]) 
				lambda[i, j, k] <- R[i,j]*exp(beta0[k] + beta1[k]*tstage[i] + beta2[k]*nstage[i]
				+beta3[k]*mstage[i]+beta4[k]*age[i]+b[k,ERfrailty[i]])*dL0[k,j]
				b[k,ERfrailty[i]]
				}
			}
		}
for(k in 1:Npairs)
	{
	for(j in 1:J)
		{
		b[j,k] ~ dnorm(0.0,tau[j])
		}
	}
for (j in 1:Td)
 {
	for (k in 1:J) 
	{ 
	dL0[k,j] ~ dgamma(mu[j,k], c)
    mu[j,k] <- dL0.star[j,k]*c
    dL0.star[j,k] <- max(0.1,r[k]*(a[j+1] - a[j]))
	}
}
for (j in 1:J)
	{
beta0[j] ~ dnorm(0,0.001)
beta1[j] ~ dnorm(0,0.001); beta2[j] ~ dnorm(0,0.001)
beta3[j] ~ dnorm(0,0.001); beta4[j] ~ dnorm(0,0.001)
tau[j]~dgamma(0.001,0.001)
	}
}

